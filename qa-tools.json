{
	"composerBinDir": "vendor\/bin",
	"projectName": "Stepup Middleware Client Bundle",
	"buildArtifacts": {
		"enabled": false
	},
	"travis": {
		"enabled": true,
		"phpVersions": {
			"1": "5.6",
			"2": "5.5",
			"3": "5.4"
		},
		"env": {
			"enabled": false
		},
		"slack": {
			"enabled": false
		}
	},
	"enablePhpTools": true,
	"enableComposer": false,
	"enablePhpLint": true,
	"enablePhpMessDetector": true,
	"phpMdExcludePatterns": [
		"*\/Tests\/*"
	],
	"enablePhpCodeSniffer": true,
	"phpCodeSnifferCodingStyle": "PSR2",
	"phpCsExcludeSymfony": false,
	"phpCsExcludeCustomPatterns": [
		"*\/Tests\/*"
	],
	"phpCsExcludePatterns": [
		"*\/Tests\/*"
	],
	"enablePhpCopyPasteDetection": true,
	"phpCpdExcludePatterns": [
		"*\/Tests\/*"
	],
	"enablePhpSecurityChecker": false,
	"phpSrcPath": [
		"src"
	],
	"enablePhpUnit": true,
	"customPhpUnitXml": false,
	"phpTestsPath": [
		"src\/Tests"
	],
	"enablePhpUnitAutoload": true,
	"phpUnitAutoloadPath": "vendor\/autoload.php",
	"enableJsTools": false,
	"enableJsHint": false,
	"enableBehat": false,
	"enablePreCommitHook": true,
	"md5Command": "md5sum",
	"_qa_tools_run_completed": true
}